{
    "name": "oro/magento-abandoned-cart",
    "description": "Oro Abandoned Cart",
    "license": "OSL-3.0",
    "authors": [
        {
            "name": "Oro, Inc",
            "homepage": "http://www.orocrm.com"
        }
    ],
    "autoload": {
        "psr-4": {"Oro\\Bundle\\AbandonedCartBundle\\": ""},
        "exclude-from-classmap": ["/Tests/"]
    },
    "require": {
        "oro/crm": "dev-releaseTests",
        "oro/mailchimp": "dev-releaseTests",
        "oro/marketing": "dev-releaseTests"
    },
    "repositories": {
        "oro": {
            "type": "composer",
            "url": "https://packagist.orocrm.com"
        },
        "asset-packagist": {
            "type": "composer",
            "url": "https://asset-packagist.orocrm.com"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-releaseTests": "3.2-dev"
        }
    }
}
